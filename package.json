{
  "name": "opencollective-website",
  "version": "0.1.164",
  "engines": {
    "node": "6.2.2",
    "npm": "3.9.5"
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "opencollective": {
    "main": "website",
    "website": {
      "entry": "./app/client",
      "dlls": [
        "opencollective-website"
      ],
      "sourceFolders": [
        "app"
      ]
    }
  },
  "scripts": {
    "start": "NODE_CONFIG_DIR=./server/config node server",
    "dev": "WEBSITE_PORT=3000 nf start -j Procfile_dev",
    "build": "node project build",
    "hint": "eslint '**/*.js' --quiet",
    "pretest": "npm run hint",
    "test": "npm run test:server && npm run test:unit",
    "test:server": "NODE_CONFIG_DIR=./server/config mocha --opts test/unit/mocha.opts test/server",
    "test:unit": "NODE_CONFIG_DIR=./server/config mocha test/unit/ --opts test/unit/mocha.opts --reporter-options mochaFile=$(bash scripts/test_output_dir.sh junit)/junit.xml",
    "posttest": "npm run test:e2e",
    "test:e2e": "npm run test:e2e:exec api:run website:run website:testE2E",
    "test:e2e:exec": "npm run rexec:api test_e2e.sh",
    "nightwatch": "NODE_CONFIG_DIR=./server/config ./node_modules/nightwatch/bin/nightwatch --config nightwatch.json",
    "test:sanity": "bash scripts/postinstall.sh",
    "git:clean": "git branch --merged | grep -v \\* | grep -v master | grep -v dev | xargs -n 1 git branch -d",
    "deploy:staging": "bash -x scripts/deploy.sh staging",
    "deploy:production": "bash -x scripts/deploy.sh production",
    "rexec:api": "bash scripts/remote_exec_api.sh",
    "install:debs": "npm run rexec:api install_apt_get_debs.sh"
  },
  "dependencies": {
    "@terse/webpack": "^1.0.3",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-preset-opencollective": "github:opencollective/babel-preset-opencollective",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "6.14.0",
    "babelify": "7.2.0",
    "basscss": "8.0.1",
    "basscss-background-images": "0.1.1",
    "browserify": "10.2.6",
    "cache-bust": "0.0.2",
    "chai": "3.5.0",
    "classnames": "2.2.4",
    "compression": "1.6.1",
    "config": "1.20.1",
    "cssnano": "3.5.2",
    "ejs": "2.4.1",
    "envify": "3.4.0",
    "express": "4.13.4",
    "express-hbs": "0.8.4",
    "express-server-status": "1.0.3",
    "express-session": "1.13.0",
    "figlet": "^1.1.2",
    "foreman": "^1.4.1",
    "gulp": "3.9.1",
    "gulp-changed": "1.3.0",
    "gulp-postcss": "6.1.1",
    "gulp-svg-sprite": "1.2.19",
    "gulp-util": "3.0.7",
    "gulp-watch": "4.3.5",
    "history": "1.17.0",
    "image-size": "0.5.0",
    "isomorphic-fetch": "2.2.1",
    "joi": "6.10.1",
    "joi-browser": "^9.0.4",
    "jwt-decode": "1.5.1",
    "jwt-simple": "0.3.1",
    "localmockage": "0.0.1",
    "lodash": "4.15.0",
    "marked": "0.3.5",
    "moment": "2.13.0",
    "morgan": "1.7.0",
    "node-emoji": "^1.4.0",
    "normalizr": "1.4.1",
    "numbro": "1.6.2",
    "postcss-cssnext": "2.5.2",
    "postcss-discard-comments": "2.0.4",
    "postcss-import": "8.1.0",
    "postcss-nested": "1.0.0",
    "query-string": "3.0.3",
    "react": "0.14.8",
    "react-datepicker": "0.23.2",
    "react-dom": "0.14.8",
    "react-onclickoutside": "4.9.0",
    "react-redux": "4.4.5",
    "react-router": "1.0.3",
    "react-stripe-checkout": "1.8.0",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-router": "1.0.0-beta8",
    "redux-thunk": "1.0.3",
    "request": "2.72.0",
    "robots.txt": "1.1.0",
    "serialize-javascript": "1.2.0",
    "serve-favicon": "2.3.0",
    "svg-to-png": "3.1.0",
    "uglify-js": "2.6.2",
    "universal-analytics": "0.3.11"
  },
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "chai": "3.5.0",
    "chai-spies": "0.7.1",
    "chromedriver": "2.21.2",
    "copy-webpack-plugin": "^3.0.1",
    "dotenv": "2.0.0",
    "ejs-loader": "^0.3.0",
    "eslint": "3.1.1",
    "eslint-config-opencollective": "1.1.4",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "5.2.2",
    "expect": "1.18.0",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "foreman": "1.4.1",
    "image-webpack-loader": "^2.0.0",
    "imports-loader": "^0.6.5",
    "jscs": "2.11.0",
    "jsdom": "3.1.2",
    "json-loader": "^0.5.4",
    "loose-envify": "1.1.0",
    "mocha": "2.4.5",
    "mocha-circleci-reporter": "0.0.1",
    "nightwatch": "0.8.18",
    "nock": "2.18.2",
    "nodemon": "1.9.2",
    "postcss-loader": "^0.11.1",
    "react-addons-test-utils": "0.14.8",
    "redux-mock-store": "1.0.2",
    "sinon": "1.17.3",
    "supertest": "1.2.0",
    "svg-sprite-loader": "0.0.27",
    "url-loader": "^0.5.7",
    "watchify": "3.7.0"
  },
  "License": "MIT"
}
